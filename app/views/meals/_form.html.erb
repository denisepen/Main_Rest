<%= render 'errors' %>

<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :calorie_count%>
<%= f.text_field :calorie_count  %> <br>
<%= f.label :description %>
<%= f.text_area :description %> <br>
<%= f.label :price%>
<%= f.number_field :price, :class => 'text_field', :step => 0.05 %> <br>
<%= f.label :category%>
<%= f.select :category, Meal.all.collect{|u| [u.category]}.uniq %> <br>


<%= f.submit  %>
